model Products {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  description String?
  price       Float
  category    String
  store       String
  barcode     String?
  brand       String?
  weight      Float?
  weightUnit  String?
  unit        String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
